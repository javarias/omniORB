XCOMM appl/test/threadtests/Imakefile

CXXSRCS = diner.cc prodcons.cc thrspecdata.cc prio.cc

DEFINES = $(OMNITHREADDEFINE)

AllTarget(ProgramTargetName(diner))
NormalCplusplusProgramTarget(diner,diner.o,$(OMNITHREADDEPEND),\
			     $(OMNITHREADLIB),NullParameter)
InstallProgram(diner,$(BINDIR))

AllTarget(ProgramTargetName(prodcons))
NormalCplusplusProgramTarget(prodcons,prodcons.o,$(OMNITHREADDEPEND),\
			     $(OMNITHREADLIB),NullParameter)
InstallProgram(prodcons,$(BINDIR))

AllTarget(ProgramTargetName(thrspecdata))
NormalCplusplusProgramTarget(thrspecdata,thrspecdata.o,$(OMNITHREADDEPEND),\
			     $(OMNITHREADLIB),NullParameter) 
InstallProgram(thrspecdata,$(BINDIR))

AllTarget(ProgramTargetName(prio))
NormalCplusplusProgramTarget(prio,prio.o,$(OMNITHREADDEPEND),\
			     $(OMNITHREADLIB),NullParameter) 
InstallProgram(prio,$(BINDIR))

DependTarget()
