<testset name="Sequence tests">

  <test name="seq long">
    <description>
    Sequence long mapping.
    </description>

    <tests>sequence base type</tests>
    <tests>sequence sequence</tests>

    <timeout>60</timeout>
    <standalone>t_seq_long</standalone>
  </test>

  <test name="seq objref">
    <description>
    Sequence objref mapping.
    </description>

    <tests>sequence objref</tests>
    <tests>sequence sequence</tests>

    <timeout>60</timeout>
    <standalone>t_seq_objref</standalone>
  </test>

  <test name="seq string">
    <description>
    Sequence string mapping.
    </description>

    <tests>sequence string</tests>
    <tests>sequence sequence</tests>

    <timeout>60</timeout>
    <standalone>t_seq_string</standalone>
