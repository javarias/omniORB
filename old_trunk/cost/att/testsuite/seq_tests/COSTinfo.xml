  <test name="string">
    <description>
    String mapping.
    </description>

    <tests>string</tests>

    <timeout>60</timeout>
    <standalone>t_string</standalone>
  </test>

  <test name="seq tests">
    <description>
    Sequence mapping and marshalling.
    </description>

    <tests>sequence base type</tests>
    <tests>sequence objref</tests>
    <tests>sequence struct</tests>
    <tests>sequence union</tests>
    <tests>sequence array</tests>
    <tests>sequence sequence</tests>

    <tests>sequence base type marshal</tests>
    <tests>sequence objref marshal</tests>
    <tests>sequence struct marshal</tests>
    <tests>sequence union marshal</tests>
    <tests>sequence array marshal</tests>
    <tests>sequence sequence marshal</tests>

    <timeout>60</timeout>
    <server>server</server>
    <client>client</client>
  </test>
</testset>
